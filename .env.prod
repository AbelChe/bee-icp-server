# 生产环境配置文件
# 请根据实际情况修改以下配置

# MySQL数据库配置
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_DATABASE=icp_database
MYSQL_USER=icp_user
MYSQL_PASSWORD=your_secure_mysql_password_here
MYSQL_PORT=3306

# 应用服务配置
APP_PORT=8600
DEBUG=false

# API密钥配置（必须配置）
CHINAZ_API_KEY=your_chinaz_api_key_here
TIANYANCHA_API_KEY=your_tianyancha_api_key_here

# 认证配置（必须修改）
API_KEY=your_very_secure_api_key_here

# 缓存配置
CACHE_EXPIRE_DAYS=30

# Nginx配置
NGINX_PORT=80
NGINX_SSL_PORT=443